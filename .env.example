# Facebook API Configuration
# Get these values from Facebook Developers: https://developers.facebook.com/

# Your Facebook Page ID (numeric)
VITE_FACEBOOK_PAGE_ID=your_page_id_here

# Your Page Access Token (long string starting with EAA...)
VITE_FACEBOOK_ACCESS_TOKEN=your_access_token_here

# Number of posts to fetch and display
VITE_FACEBOOK_POST_LIMIT=4

# Cache duration in milliseconds (900000 = 15 minutes)
VITE_FACEBOOK_CACHE_DURATION=900000

# Development Environment Password
# This password is required to access the /dev/* routes
# Copy this file to .env.local and set your actual password
# Default password if not set: dev123
VITE_DEV_PASSWORD=your_password_here
